{
  "name": "sitefactory-monorepo",
  "private": true,
  "workspaces": ["frontend", "worker"],
  "scripts": {
    "dev:frontend": "npm run dev --prefix frontend",
    "dev:worker": "npm run dev --prefix worker",
    "build": "npm run build --prefix frontend && npm run build --prefix worker",
    "migrate": "npm run migrate --prefix worker",
    "deploy:worker": "npm run deploy --prefix worker"
  }
}
